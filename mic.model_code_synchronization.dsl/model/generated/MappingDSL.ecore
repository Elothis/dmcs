<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="mappingDSL" nsURI="http://www.model_code_synchronization.mic/MappingDSL"
    nsPrefix="mappingDSL">
  <eClassifiers xsi:type="ecore:EClass" name="Model">
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
        eType="#//AbstractElement" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractElement"/>
  <eClassifiers xsi:type="ecore:EClass" name="IntegrationMechanismDeclaration" eSuperTypes="#//AbstractElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="codestructure" eType="#//Codestructure"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="modelelement" eType="#//ModelElement"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="condition" eType="#//Condition"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mapping" eType="#//Mapping"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Codestructure">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="codestructure" eType="#//CodeStructureType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="CodeStructureType">
    <eLiterals name="CLASS" literal="class"/>
    <eLiterals name="METHOD" value="1" literal="method"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ModelElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="modelelement" eType="#//ModelElementType"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ModelElementType">
    <eLiterals name="CLASS" literal="class"/>
    <eLiterals name="METHOD" value="1" literal="method"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Condition">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="condition" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Mapping">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="mapping" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
</ecore:EPackage>
